use uo;
use os;
include "include/aoslbr";
include ":logon:logon";

program logon( who )

	print( "logon: " + who.name );
//	enableAosLBRClient(who);

	if (who.title_race=="Vampire")
		who.color:=0;
		who.truecolor:=0;
	endif

	start_script( "::playermanager", who );
	if (getexistinglogons(who) == 1)
 		DisconnectClient(who);
 		return;
 	endif
endprogram